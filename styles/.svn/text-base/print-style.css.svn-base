.noPrint, #thunder
{
	display: none;
}

body
{
	background: white;
	font-family: "Times New Roman", Times, serif;
}

p, td, li
{
	font-size: 12pt;
	line-height: 14pt;
	color: black;
}

a
{
	color: black;
	text-decoration: none;
}

h1
{
	font-size: 24pt;
	line-height: 28pt;
	font-weight: bold;
}

h2
{
	font-size: 22pt;
	line-height: 26pt;
	font-weight: bold;
}

h3
{
	font-size: 20pt;
	line-height: 24pt;
	font-weight: bold;
}

h4
{
	font-size: 18pt;
	line-height: 22pt;
	font-weight: bold;
}

h5
{
	font-size: 16pt;
	line-height: 20pt;
	font-weight: bold;
}

h6
{
	font-size: 14pt;
	line-height: 18pt;
	font-weight: bold;
}

img a
{
	border: 0px;
}